<h1>New Proposal</h1>


 
<%= form_for(@proposal) do |f| %>
  <div class="field">
    <%= f.label :description %>
    <%= f.text_field :description %>
  </div>

  <div class="field">
    <%= f.label 'Bid Price' %>
    <%= f.text_field :price %>
  </div>

  <%= f.text_field :request_id,  value: 1 %>
  <%= f.text_field :service_provider_id,  value: 3 %> 
   

  <div class="actions">
    <%= f.submit 'Submit Bid' %>
  </div>
<% end %>


<%= link_to 'Back', proposals_path %>

<p id="notice"><%= notice %></p>

<h1>Service Providers</h1>

<table>
  <thead>
    <tr>
      <th>Companyname</th>
      <th>Street</th>
      <th>City</th>
      <th>Province</th>
      <th>Postalcode</th>
      <th>Email</th>
      <th>Phone</th>
      <th>Servicetype</th>
      <th>Website</th>
      <th>License</th>
      <th>Avatar</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_providers.each do |service_provider| %>
      <tr>
        <td><%= service_provider.companyName %></td>
        <td><%= service_provider.street %></td>
        <td><%= service_provider.city %></td>
        <td><%= service_provider.province %></td>
        <td><%= service_provider.postalCode %></td>
        <td><%= service_provider.email %></td>
        <td><%= service_provider.phone %></td>
        <td><%= service_provider.serviceType %></td>
        <td><%= service_provider.website %></td>
        <td><%= service_provider.license %></td>
        <td><%= service_provider.avatar %></td>
        <td><%= link_to 'Show', service_provider %></td>
        <td><%= link_to 'Edit', edit_service_provider_path(service_provider) %></td>
        <td><%= link_to 'Destroy', service_provider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service Provider', new_service_provider_path %>
